{
    "name": "match-auto-frontend",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "analyze": "ANALYZE=true next build",
        "preview": "next build && next start",
        "type-check": "tsc --noEmit",
        "pages:build": "npx @cloudflare/next-on-pages",
        "pages:deploy": "wrangler pages deploy .vercel/output/static",
        "docker:dev": "docker-compose up frontend",
        "docker:build": "docker-compose build frontend",
        "docker:prod": "docker-compose -f docker-compose.prod.yml up",
        "docker:cf-build": "docker-compose up cloudflare-build",
        "docker:clean": "docker-compose down -v && docker system prune -f",
        "setup": "npm ci && npm run docker:build",
        "fix:metadata": "node scripts/fix-metadata.js",
        "fix:types": "tsc --noEmit"
    },
    "dependencies": {
        "@clerk/nextjs": "^4.26.2",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-slot": "^1.2.4",
        "@radix-ui/react-tooltip": "^1.2.8",
        "@solana/wallet-adapter-base": "^0.9.27",
        "@solana/wallet-adapter-phantom": "^0.9.23",
        "@solana/wallet-adapter-react": "^0.15.35",
        "@solana/wallet-adapter-react-ui": "^0.9.35",
        "@solana/wallet-adapter-solflare": "^0.6.23",
        "@solana/wallet-adapter-wallets": "^0.19.32",
        "@solana/web3.js": "^1.87.6",
        "@tanstack/react-query": "^5.90.16",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "critters": "^0.0.23",
        "cross-env": "^10.1.0",
        "crypto-browserify": "^3.12.0",
        "framer-motion": "^10.16.16",
        "https-browserify": "^1.0.0",
        "lucide-react": "^0.309.0",
        "next": "15.0.0",
        "next-themes": "^0.2.1",
        "os-browserify": "^0.3.0",
        "pino-pretty": "^13.1.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-dropzone": "^14.2.3",
        "react-hot-toast": "^2.6.0",
        "sonner": "^1.4.0",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "tailwind-merge": "^2.2.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@tailwindcss/aspect-ratio": "^0.2.1",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@types/node": "^20.10.0",
        "@types/react": "^18.2.45",
        "@types/react-dom": "^18.2.18",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "@vercel/analytics": "^1.1.1",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.56.0",
        "eslint-config-next": "15.0.0",
        "postcss": "^8.4.32",
        "tailwindcss": "^4.0.0-alpha.6",
        "typescript": "^5.3.3"
    },
    "browser": {
        "crypto": "crypto-browserify",
        "stream": "stream-browserify",
        "http": "stream-http",
        "https": "https-browserify",
        "os": "os-browserify"
    }
}
