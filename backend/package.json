{
    "name": "match-auto-backend",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "wrangler dev src/index.ts",
        "build": "esbuild src/server.ts --bundle --platform=node --target=node20 --format=esm --outfile=dist/bundle.js --banner:js=\"import { createRequire } from 'module'; var require = createRequire(import.meta.url);\" --packages=external --external:lz4 --external:onnxruntime-node --external:sharp --external:http --external:https --external:fs --external:path --external:os --external:crypto --external:stream --external:util --external:events --external:url --external:zlib --external:net --external:tls --external:dns --external:buffer --external:string_decoder --external:querystring --external:worker_threads --external:hono --external:@hono/*",
        "start": "node dist/bundle.js",
        "deploy": "wrangler deploy --minify src/index.ts",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "vitest run test/e2e",
        "migrate": "tsx src/database/run-migrations.ts",
        "migrate:staging": "tsx src/database/run-migrations.ts"
    },
    "dependencies": {
        "@faker-js/faker": "^10.2.0",
        "@hono/clerk-auth": "^3.0.3",
        "@hono/node-server": "^1.19.7",
        "@hono/swagger-ui": "^0.5.3",
        "@hono/zod-validator": "^0.7.6",
        "@huggingface/inference": "^4.13.5",
        "@solana/web3.js": "^1.98.4",
        "@supabase/supabase-js": "^2.89.0",
        "@turf/turf": "^7.3.1",
        "@upstash/qstash": "^2.8.4",
        "@upstash/redis": "^1.36.0",
        "@xenova/transformers": "^2.17.2",
        "async-mutex": "^0.5.0",
        "axios": "^1.13.2",
        "crypto-js": "^4.2.0",
        "drizzle-orm": "^0.45.1",
        "esbuild": "^0.27.2",
        "hono": "^4.11.3",
        "ioredis": "^5.9.0",
        "jwt-simple": "^0.5.6",
        "kalmanjs": "^1.1.0",
        "lru-cache": "^11.2.4",
        "lz4": "^0.6.5",
        "mercadopago": "^2.11.0",
        "nodemailer": "^7.0.12",
        "onnxruntime-node": "^1.20.0",
        "p-limit": "^3.1.0",
        "pg": "^8.16.3",
        "pgvector": "^0.2.1",
        "prom-client": "^15.1.3",
        "sharp": "^0.33.5",
        "socket.io": "^4.8.3",
        "tsx": "^4.21.0",
        "ws": "^8.19.0",
        "zod": "^4.3.5"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20260103.0",
        "@prisma/client": "^7.2.0",
        "@types/crypto-js": "^4.2.2",
        "@types/ioredis": "^4.28.10",
        "@types/jwt-simple": "^0.5.36",
        "@types/nodemailer": "^7.0.4",
        "@types/pg": "^8.16.0",
        "@vitest/coverage-v8": "^4.0.16",
        "crypto-hash": "^4.0.1",
        "miniflare": "^4.20251217.0",
        "msw": "^2.12.7",
        "prisma": "^7.2.0",
        "toucan-js": "^4.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "vitest": "^4.0.16",
        "vitest-environment-miniflare": "^2.14.4",
        "wrangler": "^4.54.0"
    }
}
